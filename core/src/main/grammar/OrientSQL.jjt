{}
{
	Rid()
	|
	<LBRACKET> Rid() ( <COMMA> Rid())* <RBRACKET>
	|
	Cluster()
	|
	IndexIdentifier()
	|
    MetadataIdentifier()
	|
	Identifier()
	|
	<LPAREN> ( SelectStatement() | TraverseStatement() ) <RPAREN>
