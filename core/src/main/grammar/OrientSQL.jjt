void BinaryCondition():
{}
{
 	Expression() CompareOperator() Expression()
}

void ContainsValueCondition():
{}
{
    Expression() ContainsValueOperator()
    (
        LOOKAHEAD( 3 )
        <LPAREN> OrBlock() <RPAREN>
        |
        LOOKAHEAD( Expression() )
        Expression()
    )
}

void InstanceofCondition():
{}
{
    Expression() <INSTANCEOF> ( Identifier() | <STRING_LITERAL> )
}

void IndexMatchCondition():
{}
{
 	<KEY>
 	(
 	    CompareOperator() <LBRACKET> [ Expression() ( <COMMA> Expression() )* ] <RBRACKET>
 	    |
 	    <BETWEEN> <LBRACKET> [ Expression() ( <COMMA> Expression() )* ] <RBRACKET> <AND> <LBRACKET> [ Expression() ( <COMMA> Expression() )* ] <RBRACKET>
 	)
}

void BetweenCondition():
{}
{
	Expression() <BETWEEN> Expression() <AND> Expression()
}

void IsNullCondition():
{}
{
	Expression() <IS> <NULL>
}

void IsNotNullCondition():
{}
{
	Expression() <IS> <NOT> <NULL>
}

void IsDefinedCondition():
{}
{
	Expression() <IS> <DEFINED>
}

void IsNotDefinedCondition():
{}
{
	Expression() <IS> <NOT> <DEFINED>
}

void ContainsCondition():
{}
{
	Expression() <CONTAINS>

	(
	    LOOKAHEAD( 3 )
	    ( <LPAREN> OrBlock() <RPAREN> )
	    |
	    LOOKAHEAD( Expression() )
	    Expression()
	)

}

void InOperator():
{}
{
    <IN>
}

void InCondition():
{}
{
	Expression() InOperator()
	(
	    <LBRACKET> Expression() (<COMMA> Expression())* <RBRACKET>
	    |
	    <LPAREN> SelectStatement() <RPAREN>
	    |
	    InputParameter()
	)
}

void NotInCondition():
{}
{
	Expression() <NOT> InOperator()
	(
	    <LBRACKET> Expression() (<COMMA> Expression())* <RBRACKET>
        |
        <LPAREN> SelectStatement() <RPAREN>
	)
}

void ContainsAllCondition():
{}
{
	Expression() <CONTAINSALL> <LPAREN> OrBlock() <RPAREN>
}

void ContainsTextCondition():
{}
{
	Expression() <CONTAINSTEXT> Expression()
}

void MatchesCondition():
{}
{
	Expression() <MATCHES> <STRING_LITERAL>
}

